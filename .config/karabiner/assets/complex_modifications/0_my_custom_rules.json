{
  "title": "My Custom Rules",
  "rules": [
    {
      "description": "Return when left_control + j",
      "manipulators": [
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Return when right_control + j",
      "manipulators": [
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "right_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Return when caps_lock + j",
      "manipulators": [
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "up_arrow when left_control + p",
      "manipulators": [
        {
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "dow_arrow when left_control + n",
      "manipulators": [
        {
          "from": {
            "key_code": "n",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "left_arrow when left_control + f (only finder)",
      "manipulators": [
        {
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "type": "basic",
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.finder"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "right_arrow when left_control + b (only finder)",
      "manipulators": [
        {
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "type": "basic",
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.finder"
              ]
            }
          ]
        }
      ]
    }
  ]
}